    <tr>
      <th>Legal affair</th>
      <th>Lawyer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lawyer_legal_affairs.each do |lawyer_legal_affair| %>
      <tr>
        <td><%= lawyer_legal_affair.legal_affair_id %></td>
